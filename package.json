{
  "name": "moodchat-backend",
  "version": "1.0.0",
  "description": "Real-time chat backend with Express, Socket.IO, PostgreSQL, and Redis",
  "main": "src/server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "dev:debug": "cross-env NODE_ENV=development nodemon --inspect src/server.js",
    "prod": "cross-env NODE_ENV=production node src/server.js",
    "prod:cluster": "cross-env NODE_ENV=production node src/cluster.js",
    "test": "cross-env NODE_ENV=test jest --coverage --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:e2e": "cross-env NODE_ENV=test jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.js --fix",
    "lint:check": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:migrate:status": "sequelize-cli db:migrate:status",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "docker:build": "docker build -t moodchat-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env moodchat-backend",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:outdated": "npm outdated",
    "security:check": "npm audit && npm outdated",
    "build": "echo ? Build completed successfully!",
    "prestart": "npm run build",
    "docker:build:prod": "docker build -f Dockerfile.prod -t moodchat-backend:prod .",
    "docker:push": "docker tag moodchat-backend:prod your-registry/moodchat-backend:latest && docker push your-registry/moodchat-backend:latest",
    "health:check": "curl -f http://localhost:3001/health || exit 1",
    "metrics": "curl http://localhost:3001/metrics",
    "docs:generate": "apidoc -i src/ -o docs/",
    "docs:serve": "http-server docs/ -p 8080",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "benchmark": "autocannon -c 100 -d 30 http://localhost:3001/health",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install"
  },
  "keywords": [
    "chat",
    "realtime",
    "socket.io",
    "express",
    "postgresql",
    "redis",
    "jwt",
    "websocket"
  ],
  "author": "MoodChat Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/moodchat-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/moodchat-backend/issues"
  },
  "homepage": "https://github.com/your-org/moodchat-backend#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.440.0",
    "@aws-sdk/s3-request-presigner": "^3.440.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "argon2": "^0.30.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.21",
    "mongoose": "^9.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.16.1",
    "nanoid": "^5.0.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.12",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.17.1",
    "pg-hstore": "^2.3.4",
    "pm2": "^6.0.14",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.3",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@faker-js/faker": "^8.3.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "apidoc": "^0.29.0",
    "autocannon": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^1.7.1",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/tests/**",
      "!src/server.js",
      "!src/cluster.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "setupFilesAfterEnv": [
      "./src/tests/setup.js"
    ]
  },
  "volta": {
    "node": "18.18.2",
    "npm": "9.8.1"
  },
  "files": [
    "src/",
    "package.json",
    "README.md",
    "LICENSE",
    ".env.example",
    "Dockerfile",
    "docker-compose.yml",
    "ecosystem.config.js"
  ],
  "contributors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/your-username"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "optionalDependencies": {
    "sharp": "^0.33.2"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
